		"nyc": "^14.1.1",
		"standard-version": "^6.0.1"
	},
	"main": "index.js",
	"scripts": {
		"test": "mocha --timeout 10000 generators/test/app/*.test.js generators/test/app/**/*.test.js --reporter spec",
